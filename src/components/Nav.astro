---
const {logo} = Astro.props
---
<nav class="w-full py-5 bg-zinc-800 flex px-4">
    <a class="text-zinc-100" href="#">{logo}</a>
    <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
